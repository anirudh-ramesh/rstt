<?xml version="1.0"?>
<block>
    <name>Symbols2bits</name>
    <key>rstt_symbols2bits</key>
    <category>RSTT</category>
    <import>import rstt</import>
    <make>rstt.symbols2bits($sync_nbits)</make>
    <param>
        <name>Sync window size (bits)</name>
        <key>sync_nbits</key>
        <value>20*10</value>
        <type>int</type>
    </param>
    <sink>
        <name>in</name>
        <type>byte</type>
    </sink>
    <source>
        <name>out</name>
        <type>byte</type>
    </source>
</block>
