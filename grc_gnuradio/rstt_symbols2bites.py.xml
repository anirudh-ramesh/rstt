<?xml version="1.0"?>
<block>
    <name>RSTT -  symbols to bites</name>
	<key>rstt_symbols2bites</key>
	<category>RSTT</category>
	<import>from gr.symbols2bites import symbols2bites</import>
	<make>symbols2bites($sync_nbites)</make>
	<param>
		<name>Sync window lenght</name>
		<key>sync_nbites</key>
		<value>20*10</value>
		<type>int</type>
	</param>
	<sink>
		<name>in</name>
		<type>byte</type>
	</sink>
	<source>
		<name>out</name>
		<type>byte</type>
	</source>
</block>

